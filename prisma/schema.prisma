generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DB_URL")
}

model User {
    id        String   @id @default(uuid())
    username  String   @unique
    email     String   @unique
    fullname  String
    password  String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
